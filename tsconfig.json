{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "noImplicitAny": false,
    "paths": {
        "@components/*": ["src/components/*"],
        "@layouts/*": ["src/layouts/*"],
        "@pages/*": ["src/pages/*"],
        "@styles/*": ["src/styles/*"],
        "@static/*": ["static/*"],
        "@models/*": ["src/models/*"],
        "@consts": ["src/consts.ts"],
        "@utils": ["src/utils.ts"],
    }
  }
}
